<link rel="stylesheet" href="<%=assetPath('css/user_sign_in.css')%>">

<div class="outer">
    <div class="sign sign-in-body">
            <h1>Sign In</h1>
            <form action="/users/create-session" method="post" id="sign-in">
                    
                    <input type="email" name="email" required placeholder="Email or Password"><br>
                    
                    <input type="password" name="password" placeholder="Password"required><br>

                    <button type="submit" form="sign-in">Log In</button> <p>New User ?? <a href="/users/sign-up">Sign Up</a></p>
            </form>

            <p>
                    <a href="/users/auth/google">google Sign In/sign up</a>
            </p>
    </div>
</div>